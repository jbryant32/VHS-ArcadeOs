<Application x:Class="LaunchPad.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LaunchPad"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="PacFonts">pack://application:,,,/Fonts/#PacFont</FontFamily>
        <FontFamily x:Key="BadMofo">pack://application:,,,/Fonts/#Bad Mofo</FontFamily>
    </Application.Resources>
</Application>
